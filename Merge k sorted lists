# Definition for singly-linked list.
# class ListNode:
#     def __init__(self, val=0, next=None):
#         self.val = val
#         self.next = next
import heapq
class Solution:
    def mergeKLists(self, lists: List[Optional[ListNode]]) -> Optional[ListNode]:
        queue = []
        count = 0
        for lis in lists:
            root = lis
            while root:
                heapq.heappush(queue,(root.val,count,root))
                count+=1
                root=root.next
        dummy = ListNode(0)
        cur = dummy
        for i in range(len(queue)):
            node_val,count,node = heapq.heappop(queue)
            cur.next = node
            cur = cur.next
        return dummy.next





        
