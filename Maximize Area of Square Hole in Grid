class Solution:
    def maximizeSquareHoleArea(self, n: int, m: int, hBars: List[int], vBars: List[int]) -> int:
        #given - n,m hbars and vbars , grid of n+2 and m+2 , return max square are after remove bars or none
        hBars.sort()
        vBars.sort()
        start = 1
        h,v = 1,1
        for i in range(1,len(hBars)):
            if hBars[i]==hBars[i-1]+1: #get consective h
                
                start+=1
            else:
                h = max(h,start)
                start = 1
        h = max(h, start)

        start = 1
        for j in range(1,len(vBars)):
            if vBars[j]==vBars[j-1]+1: #get consective v
                
                start+=1
            else:
                v = max(v,start)
                start=1
        v = max(v,start)
        side = min(h,v)+1 #get area from side
        return side*side


        
