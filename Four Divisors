class Solution:
    def sumFourDivisors(self, nums: List[int]) -> int:
        total_sum = 0
        def find_divisors(num): #divisors always occure in pairs
            lis = []
            i = 1
            while i*i<=num:
                if num%i==0: #if divisors append
                    lis.append(i)
                    if i!=num//i: #if not same to prev divisiors
                        lis.append(num//i)
                i+=1
            return lis


        for num in nums:
            divisors = find_divisors(num)
            if len(divisors)==4:
                total_sum+=sum(divisors)
        return total_sum
