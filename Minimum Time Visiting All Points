class Solution:
    def minTimeToVisitAllPoints(self, points: List[List[int]]) -> int:
        n = len(points)
        steps = 0
        for i in range(n-1):
            x1,y1 = points[i][0],points[i][1]
            x2,y2 = points[i+1][0],points[i+1][1]
            dx = abs(x1-x2)
            dy = abs(y1-y2)
            steps+=(min(dx,dy)+abs(dx-dy)) #get dist other than diagiaonal
        return steps
        
